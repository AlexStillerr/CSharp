<UserControl x:Class="MatchFour.UserControlls.InsertButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MatchFour.UserControlls"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="70">
    <Grid ClipToBounds="True">
        <Rectangle Fill="#FF115696"/>
        <Ellipse Fill="White" RenderTransformOrigin="0.5,0.5" Margin="5,-27,5,15" Width="50" Height="50"/>
        <Button BorderThickness="0" Background="Transparent" Click="Button_Click" Panel.ZIndex="3"/>
    </Grid>
</UserControl>
